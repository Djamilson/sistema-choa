{
  "name": "backend",
  "version": "3.0.0",
  "private": true,
  "description": "API Shooferta Shop",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "vercel-build": "npx prisma migrate deploy",
    "build-tsup": "tsup src",
    "dev:server:ts-node": "ts-node-dev --trace-warnings --files -r tsconfig-paths/register --exit-child --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "dev:server": "tsx --watch src/shared/infra/http/index.ts",
    "start": "ts-node src/shared/infra/http/index.ts",
    "dev:test-carga": "npx autocannon -c 500 -d 30 --workers 10 --renderStatusCodes --latency --warmup [ -c 1 -d 2 ] localhost:3000",
    "test:jest": "jest",
    "test": "node --test test/",
    "test:coverage": "node --test --experimental-test-coverage test/",
    "test:debug": "node --inspect --watch --test test/",
    "dev:server:climem": "CLIMEM=8999 tsx -r climem watch src/shared/infra/http/index.ts",
    "climem": "npx climem 8999",
    "prisma:migrate:postgres:dev": "prisma migrate dev --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:generate:postgres": "prisma generate --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:pull:postgres": "prisma db pull --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:migrate:postgres:deploy": "prisma migrate deploy --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:migrate:mysql:dev": "prisma migrate dev --schema=src/shared/infra/prisma/mysql/schema-mysql.prisma",
    "prisma:generate:mysql": "prisma generate --schema=src/shared/infra/prisma/mysql/schema-mysql.prisma",
    "prisma:migrate:mysql:deploy": "prisma migrate deploy --schema=src/shared/infra/prisma/mysql/schema-postgres.prisma",
    "prisma:generate:mongodb": "prisma generate --schema=src/shared/infra/prisma/mongodb/schema-mongodb.prisma",
    "prisma:migrate:mongodb": "prisma db push --schema=src/shared/infra/prisma/mongodb/schema-mongodb.prisma",
    "prisma:introspect": "prisma introspect",
    "prisma:seed:postgres": "prisma db seed --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:studio": "prisma studio --experimental",
    "prisma:postgres:format": "prisma format --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma"
  },
  "prisma": {
    "schema-postgres": "npx tsx src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "schema-mongodb": "npx tsx src/shared/infra/prisma/mongodb/schema-mongodb.prisma",
    "seed": "npx tsx src/shared/infra/prisma/postgres/dataDefault/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@splidejs/splide": "^4.1.4",
    "add": "^2.0.6",
    "aws-sdk": "^2.1379.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "brasilapi": "^1.0.0",
    "bson": "^4.6.2",
    "celebrate": "^15.0.3",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "cuid": "^3.0.0",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.9",
    "dayjs-plugin-utc": "^0.1.2",
    "es6-shim": "^0.35.6",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "googleapis": "^137.1.0",
    "handlebars": "^4.7.6",
    "ioredis": "^5.3.2",
    "ip": "^1.1.8",
    "json-formatter-js": "^2.3.4",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "natural": "^6.2.0",
    "nodemailer": "^6.7.7",
    "pg": "^8.7.3",
    "qs": "^6.10.3",
    "rate-limiter-flexible": "^2.3.6",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "resend": "^3.2.0",
    "sentiment": "^5.0.2",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.2",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mime": "^2.0.3",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.3",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autocannon": "^7.15.0",
    "babel-plugin-const-enum": "^1.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "climem": "^2.0.0",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "jest": "^27.5.1",
    "pagarme-js-types": "^2.9.7",
    "prettier": "^3.2.5",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.17.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.15.4",
    "typescript": "^5.4.5"
  }
}
